<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AdvancedImageProcessing.Views.ImageRotation">
	<StackPanel>
		<StackPanel Grid.Row="0" Grid.ColumnSpan="2" Orientation="Horizontal">
			<Button Margin="4 0 2 0">開啟檔案</Button>
			<Button Margin="4 0 2 0">儲存檔案</Button>
			<Button Margin="4 0 2 0">逆時針旋轉</Button>
			<Button Margin="4 0 2 0">順時針旋轉</Button>
		</StackPanel>
		<Grid ColumnDefinitions="*,*" ShowGridLines="True">
			<Image Grid.Column="0" Source="D:\Repository\AdvancedImageProcessing\rat.png"></Image>
			<Image Grid.Column="1" Source="D:\Repository\AdvancedImageProcessing\rat.png"></Image>
		</Grid>
	</StackPanel>
</UserControl>
